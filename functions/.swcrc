{
    "$schema": "https://json.schemastore.org/swcrc",
    "minify": true,
    "jsc": {
        "target": "esnext",
        "parser": {
            "syntax": "typescript",
            "dynamicImport": true
        },
        "minify": {
            "compress": {
                  "unused": true
            },
            "mangle": true
        }
    },
    "module": {
        "type": "commonjs",
        "strict": true,
        "strictMode": true,
        "lazy": true
    },
    "sourceMaps": true
}